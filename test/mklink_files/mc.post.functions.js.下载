$(document).on("mouseenter",".common-center .postname h5",function(){$(this).parent().next().hide()}),$(document).on("mouseleave",".common-center .postname h5",function(){$(this).parent().next().show()}),$(document).on("click",".common-fuc-group .push",function(){var t={},n=$(this);t.todo="pushpost",t.post_id=$(this).parent().parent().attr("data-id");var e=JSON.stringify(t),a=$.ajax({url:"/action/doPost/",async:!0,type:"post",data:{data:e},beforeSend:function(){},complete:function(){if(a.responseText){var t=$.parseJSON(a.responseText);if(0==t.state){common_msg(PublicLangData.remind,PublicLangData.post.push.complete,"ok");var e=n.find("i"),o=n.children(".nums"),s=parseInt(o.attr("title")),i=n.find("span");e.hasClass("far")&&(e.removeClass("far").addClass("fas"),o.text(s+1).attr("title",s+1),i.text(PublicLangData.post.push.off))}else common_msg(PublicLangData.warning.inform.title,PublicLangData.warning.inform[t.state],"err")}},error:function(t,n){connect_error(n)},timeout:timmeLimit})});